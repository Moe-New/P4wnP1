#
USB_VID="0x1D6B"        # Vendor ID
USB_PID="0x0237"        # Product ID

USE_ECM=false            # kept enabled as fallback (could allow forwarding connection trough WiFi if added to payload)
USE_RNDIS=true          # kept enabled as fallback (could allow forwarding connection trough WiFi if added to payload)
USE_HID=false
USE_RAWHID=false        
USE_UMS=false

# disable setting of static routes for all IPv4 addresses
ROUTE_SPOOF=false

WIFI_ACCESSPOINT=true
WIFI_ACCESSPOINT_NAME="Big-Brother"
WIFI_ACCESSPOINT_AUTH=true # Use WPA2_PSK if true, no authentication if false
WIFI_ACCESSPOINT_CHANNEL=6
WIFI_ACCESSPOINT_PSK="is-watching-you"
WIFI_ACCESSPOINT_IP="172.24.0.1" # IP used by P4wnP1
WIFI_ACCESSPOINT_NETMASK="255.255.255.0"
WIFI_ACCESSPOINT_DHCP_RANGE="172.24.0.2,172.24.0.10" # DHCP Server IP Range
WIFI_ACCESSPOINT_HIDE_SSID=false # use to hide SSID of WLAN (you have to manually connect to the name given by WIFI_ACCESSPOINT_NAME)

WIFI_CLIENT=true
WIFI_CLIENT_SSID="Satori" # name of target network
WIFI_CLIENT_PSK="satorare" # passphrase for target network

AUTOSSH_ENABLED=false # enable AutoSSH
AUTOSSH_REMOTE_HOST="remoteSSHServer.com"
AUTOSSH_REMOTE_USER=root 
AUTOSSH_PRIVATE_KEY="$wdir/ssh/keys/P4wnP1_id"
AUTOSSH_PUBLIC_KEY="$wdir/ssh/keys/P4wnP1_id.pub" 
AUTOSSH_REMOTE_PORT=8765 
